<p id="notice"><%= notice %></p>

<h1>Listing Issues</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Created at</th>
      <th>Neighborhood</th>
      <th>Street name</th>
      <th>Acknowledged</th>
      <th>Rtid</th>
      <th>Rtt</th>
      <th>Adress</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Id </th>
      <th>Integer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issues.each do |street_name,count| %>
      <tr>
        <td><%= street_name %></td>
        <td><%= count %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issue', new_issue_path %>
